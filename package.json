{
  "name": "node-api",
  "version": "1.0.0",
  "description": "node api",
  "main": "src/index.js",
  "scripts": {
    "transpile": "babel ./src --out-dir build",
    "clean": "rimraf build",
    "build": "npm-run-all clean transpile",
    "server": "node ./build/index.js",
    "dev": "NODE_ENV=development npm-run-all build server",
    "prod": "NODE_ENV=production npm-run-all build server",
    "watch:dev": "nodemon",
    "start": "npm run prod",
    "start:dev": "nodemon --exec babel-node src/index.js"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/*",
      "openApiDocumentation.json",
      "views/*.ejs"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "author": "KMTC",
  "license": "ISC",
  "dependencies": {
    "@babel/register": "^7.8.3",
    "@paypal/checkout-server-sdk": "^1.0.2",
    "axios": "^0.19.2",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "ejs-mate": "^3.0.0",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "express-session": "^1.17.1",
    "googleapis": "^55.0.0",
    "ip": "^1.1.5",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.27.0",
    "morgan": "^1.9.1",
    "nodemailer": "^6.4.10",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "paypal-rest-sdk": "^1.8.1",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.5",
    "stripe": "^8.76.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.2"
  }
}
