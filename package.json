{
  "name": "node-api",
  "version": "1.0.0",
  "description": "node api",
  "main": "src/index.js",
  "scripts": {
    "transpile": "babel ./src --out-dir build",
    "clean": "rimraf build",
    "build": "npm-run-all clean transpile",
    "server": "node ./build/index.js",
    "dev": "NODE_ENV=development npm-run-all build server",
    "prod": "NODE_ENV=production npm-run-all build server",
    "watch:dev": "nodemon",
    "start": "npm run prod",
    "start:dev": "nodemon --exec babel-node src/index.js"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "author": "KMTC",
  "license": "ISC",
  "dependencies": {
    "@babel/register": "^7.8.3",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "npm-run-all": "^4.1.5",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.5",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.2"
  }
}
